{
  "name": "@ionic-enterprise/identity-vault",
  "version": "5.9.5",
  "description": "Ionic Identity Vault Plugin",
  "author": "Ionic Team <hi@ionic.io> (https://ionicframework.com) ",
  "license": "SEE LICENSE",
  "cordova": {
    "id": "@ionic-enterprise/identity-vault",
    "platforms": [
      "android",
      "ios"
    ]
  },
  "files": [
    "dist",
    "plugin.xml",
    "src/android",
    "src/ios",
    "www"
  ],
  "scripts": {
    "build": "npm run clean && npm run transpile && npm run rollup && npm run downlevel-dts",
    "clean": "rimraf dist",
    "clean-change-log": "node ../../scripts/clean-changelog.js CHANGELOG.md",
    "remove-void-implements": "node ../../scripts/remove-void-implements.js",
    "dev": "npm run clean && npm-watch transpile & sleep 3 && npm-watch rollup",
    "prepublishOnly": "npm run build",
    "rollup": "rollup --config rollup.config.js",
    "transpile": "tsc",
    "docs": "typedoc --plugin typedoc-plugin-markdown --theme ../../node_modules/@ionic/markdown-docs-theme/dist src/index.ts && rm -rf ../../website/docs/{enums,interfaces,classes} && npm run remove-void-implements && mv api/{enums,interfaces,classes} ../../website/docs && mv api/modules.md ../../website/docs/modules.md && rm -rf api",
    "release": "standard-version -a",
    "pre-release": "standard-version -a -p --skip.changelog=true --skip.tag=true",
    "downlevel-dts": "npx downlevel-dts dist/typings dist/typings/ts_old --to=3.5",
    "test": "echo no tests yet"
  },
  "watch": {
    "transpile": {
      "patterns": [
        "src"
      ],
      "extensions": "ts,js",
      "quite": false
    },
    "rollup": "dist/esm/**/*.*"
  },
  "main": "dist/index.js",
  "typings": "dist/typings/index.d.ts",
  "typesVersions": {
    "<4.1": {
      "dist/typings/index.d.ts": [
        "dist/typings/ts_old/index.d.ts"
      ]
    }
  },
  "devDependencies": {
    "@ionic/markdown-docs-theme": "3.0.1",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@types/cordova": "^0.0.34",
    "concat-md": "0.3.5",
    "conventional-changelog": "^3.1.4",
    "downlevel-dts": "^0.7.0",
    "npm-watch": "^0.9.0",
    "replace": "^1.1.0",
    "rimraf": "^2.6.1",
    "rollup": "^2.40.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "sync-cordova-xml2": "^0.0.3",
    "typedoc": "0.20.30",
    "typedoc-plugin-markdown": "3.6.0",
    "typescript": "4.1.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ionic-team/enterprise-auth.git"
  },
  "bugs": {
    "url": "https://ionic.zendesk.com/hc/en-us"
  },
  "publishConfig": {
    "access": "restricted",
    "registry": "https://registry.npmjs.org"
  },
  "engines": {
    "cordovaDependencies": {
      "3.0.0": {
        "cordova-android": ">=6.3.0"
      },
      "4.0.0": {
        "cordova-android": ">=9.0.0-dev"
      },
      "5.0.0": {
        "cordova-android": ">=10.0.0",
        "cordova": ">=10.0.0"
      }
    }
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": [
      "**/__test__/*.(ts|tsx|js)"
    ]
  },
  "dependencies": {
    "tslib": "^1.9.0"
  },
  "peerDependencies": {
    "typescript": ">=3.5"
  },
  "gitHead": "968f09d9b766befc7fe767242d1237e197665ec9"
}
