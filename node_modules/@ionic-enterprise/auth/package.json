{
  "name": "@ionic-enterprise/auth",
  "version": "3.10.2",
  "description": "Ionic Enterprise Cordova Auth Plugin",
  "author": "Ionic Team <hi@ionic.io> (https://ionicframework.com)",
  "license": "SEE LICENSE",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ionic-team/cordova-ionic-auth.git"
  },
  "cordova": {
    "id": "@ionic-enterprise/auth",
    "platforms": [
      "android",
      "ios"
    ]
  },
  "files": [
    "dist",
    "plugin.xml",
    "src/android",
    "src/ios"
  ],
  "scripts": {
    "build": "npm run clean && npm run transpile && npm run rollup",
    "clean": "rimraf dist",
    "clean-change-log": "node ../../scripts/clean-changelog.js CHANGELOG.md",
    "cordova-sync": "sync-cordova-xml2 package.json plugin.xml --output=plugin.xml",
    "dev": "npm run clean & npm-watch transpile & sleep 3 && npm-watch rollup",
    "docs": "typedoc --plugin typedoc-plugin-markdown --theme ./node_modules/@ionic/markdown-docs-theme/dist src/index.ts && rm -rf ../../website/docs/{classes,interfaces} && mv api/{classes,interfaces} ../../website/docs && rm -rf api",
    "rollup": "rollup -c rollup.config.js",
    "test": "jest --maxWorkers=4",
    "transpile": "tsc"
  },
  "watch": {
    "transpile": {
      "patterns": [
        "src"
      ],
      "extensions": "ts,tsx",
      "quite": false
    },
    "rollup": "dist/esm/**/*.*"
  },
  "dependencies": {
    "base64-js": "^1.3.0",
    "lodash-es": "^4.17.20",
    "tslib": "^2.0.3",
    "url-parse": "^1.5.0"
  },
  "devDependencies": {
    "@ionic/markdown-docs-theme": "3.0.1",
    "@rollup/plugin-commonjs": "^12.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@types/base64-js": "^1.2.5",
    "@types/cordova": "0.0.34",
    "@types/jest": "^25.2.3",
    "@types/lodash-es": "^4.17.4",
    "@types/node-fetch": "^2.3.3",
    "@types/url-parse": "^1.4.3",
    "concat-md": "^0.3.5",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "jest-cli": "^26.0.1",
    "npm-watch": "^0.7.0",
    "prettier": "^2.3.0",
    "rimraf": "^2.6.2",
    "rollup": "^2.12.1",
    "rollup-plugin-sourcemaps": "0.6.2",
    "sync-cordova-xml2": "^0.0.3",
    "ts-jest": "^26.0.0",
    "typedoc": "0.20.30",
    "typedoc-plugin-markdown": "3.6.0",
    "typescript": "^4.1.5"
  },
  "main": "dist/index.js",
  "types": "dist/esm/index.d.ts",
  "gitHead": "5a3ce449975294d9c4780b99471e962e0a052f37"
}
